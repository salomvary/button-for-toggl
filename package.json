{
  "name": "button-for-toggl",
  "version": "0.0.1",
  "description": "Add Toggl one-click time tracking to popular web tools.",
  "license": "BSD-3-Clause",
  "private": true,
  "engines": {
    "node": ">=10.14.0"
  },
  "scripts": {
    "start": "webpack-cli --mode development --watch",
    "build": "webpack-cli --mode production --env production",
    "release": "webpack-cli --mode production --env production --env release",
    "lint": "eslint",
    "lint:staged": "lint-staged",
    "postversion": "npm run release"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-transform-react-jsx"
      ]
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "Firefox ESR"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/scripts/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.24.3",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-transform-react-jsx": "7.23.4",
    "@babel/preset-env": "7.24.3",
    "@babel/preset-react": "7.24.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.8",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "babel-loader": "8.3.0",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "6.1.2",
    "copy-webpack-plugin": "6.4.1",
    "eslint": "5.12.1",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-preact": "1.1.6",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.0",
    "filemanager-webpack-plugin": "3.1.1",
    "fs-extra": "8.0.1",
    "husky": "2.4.0",
    "lint-staged": "8.2.0",
    "raw-loader": "4.0.2",
    "semantic-release": "17.3.0",
    "svg-url-loader": "7.1.1",
    "ts-loader": "7.0.5",
    "typescript": "3.5.1",
    "web-ext": "7.8.0",
    "webextension-polyfill": "0.4.0",
    "webpack": "5.91.0",
    "webpack-cli": "4.10.0",
    "webpack-log": "3.0.2"
  },
  "dependencies": {
    "@emotion/core": "10.0.10",
    "@emotion/styled": "10.0.10",
    "@toggl/track-extension": "github:toggl/track-extension",
    "date-fns": "1.30.1",
    "keycode": "2.2.0",
    "lodash.map": "4.6.0",
    "lodash.reduce": "4.6.0",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-emotion": "10.0.0"
  }
}
